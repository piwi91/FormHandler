{
    "name": "piwi91/form-handler",
    "description": "A Form Handler implementation for the Symfony Form Component",
    "type": "library",
    "require": {
        "symfony/http-foundation": "^2.3",
        "symfony/form": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Piwi\\Form\\": ["src/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Piwi\\Form\\": ["src/", "tests/fixture/"]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Pim Widdershoven",
            "email": "pim@widdershoven.nl"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "mockery/mockery": "^0.9.4"
    }
}
